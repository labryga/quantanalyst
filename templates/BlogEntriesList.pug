{% extends "base.pug" %}

{% block navigaton_desktop_tablet %}
for mnuitem in menu_list
	li: a(href="{{ request.scheme }}://{{ request.META.HTTP_HOST }}/{{ mnuitem.menu_title | lower }}") {{ mnuitem.menu_title }}
{% endblock navigaton_desktop_tablet %}



{% block content %}
ul
	for blogitem in object_list
		li: a(href="{{ request.scheme }}://{{ request.META.HTTP_HOST }}/{{ blogitem.category }}/{{ blogitem.slug }}") {{ blogitem.titel | striptags }}
{% endblock content %}

